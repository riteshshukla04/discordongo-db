{
  "name": "discordongo-express-example",
  "version": "1.0.0",
  "description": "Express.js example using DiscordDB as backend",
  "type": "module",
  "main": "express-app.js",
  "scripts": {
    "dev": "tsx watch express-app.ts",
    "build": "tsc express-app.ts --outDir dist --target es2022 --module es2022 --moduleResolution node",
